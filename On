DRAG=-1
end

function box2()
PROGRAM = gg.choice({
"drag 1",
"drag 2",
"drag 3",
"menu 4"},nil,"gunakan mobil ek9 lalu set bumper dan setelah itu edit di mobil yang mau di setting bumper")
if PROGRAM == 1 then drag1() end
if PROGRAM == 2 then drag2() end
if PROGRAM == 3 then drag3() end
if PROGRAM == 4 then drag4() end
end

function drag1()
