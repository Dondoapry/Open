function raceframe()
MODE = gg.choice({

"💿ONstartGravity",
"📀ONstartRaceGo",
"💿ONcustomSpeed",
"📀OFFstartRaceGo",
"💿OFFvalueRace",
"📀StartAttackRace",
"💿ModeRaceTunupStandard",
"📀ModeRaceFly402m",
"💿ModeRaceFly1600m",
"📀ModeRaceFly3km",
"💿ModeRaceLiaran",
"📀ModeRaceMelawanNanTimeON",
"💿ModeRaceMelawanNanTimeOFF",
"📀ModeRaceNormalLiaranRepireCar",

"Exit"},nil,"🇮🇩PROGRAM BETA # CAR PARKING MULTIPLAYER INDONESIA🇮🇩")

if MODE == 1 then ONstartGravity() end
if MODE == 2 then ONstartRaceGo() end
if MODE == 3 then ONcustomSpeed() end
if MODE == 4 then OFFstartRaceGo() end
if MODE == 5 then OFFvalueRace() end
if MODE == 6 then StartAttackRace() end
if MODE == 7 then ModeRaceTunupStandard() end
if MODE == 8 then ModeRaceFly402m() end
if MODE == 9 then ModeRaceFly1600m() end
if MODE == 10 then ModeRaceFly3km() end
if MODE == 11 then ModeRaceLiaran() end
if MODE == 12 then ModeRaceMelawanNanTimeON() end
if MODE == 13 then ModeRaceMelawanNanTimeOFF() end
if MODE == 14 then ModeRaceNormalLiaranRepireCar() end
if MODE == 15 then Exit() end
end


