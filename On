if menu == nil then 
DATA = menu[1]
end                     --1                          --2                   --3                -4     --5                    -6                      
local bini1={"-1093894483","-1093884483","-1094874483","0","1065353216","1073741824","1077936128","1082130432","1084227584","1086324736","1088421888","1090519040","1091567616","1092616192","1094713344"}
