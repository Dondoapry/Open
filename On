if ch == 2 then editing() end
if ch == 3 then editingnorm() end
if ch == 4 then editingnorm() end
end

function chit()
gg.setRanges(gg.REGION_CODE_APP)
input=gg.prompt({'MASUKAN HP SPECK'}, 
{[1]='0'}, 
{[1]='number'}) 
if input==nil then 
os.exit() 
end
gg.searchNumber(input[1],gg.TYPE_FLOAT)
gg.getResults(1000)
gg.editAll('1999', gg.TYPE_FLOAT)
gg.clearResults()
input=gg.prompt({'UBAH HP SPECK'}, 
{[1]='0'}, 
{[1]='number'}) 
if input==nil then 
os.exit() 
end
gg.searchNumber(input[1],gg.TYPE_FLOAT)
gg.getResults(1000)
gg.editAll('2999', gg.TYPE_FLOAT)
gg.clearResults()
input=gg.prompt({'MASUKAN TORSI HP'}, 
{[1]='0'}, 
{[1]='number'}) 
if input==nil then 
os.exit() 
end
gg.searchNumber(input[1],gg.TYPE_FLOAT)
gg.getResults(1000)
gg.editAll('1000', gg.TYPE_FLOAT)
gg.clearResults()
input=gg.prompt({'UBAH TORSI HP'}, 
{[1]='0'}, 
{[1]='number'}) 
if input==nil then 
os.exit() 
end
gg.searchNumber(input[1],gg.TYPE_FLOAT)
gg.getResults(1000)
gg.editAll('1011', gg.TYPE_FLOAT)
gg.clearResults()
gg.alert('TUNGGU LOADING DAN RESET TUNNING')
end

