function ModeRaceNormalLiaranRepireCar()
gg.setVisible(false)
gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber('-99999',gg.TYPE_FLOAT)
gg.getResults('99999')
gg.editAll('2500',gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('10',gg.TYPE_FLOAT)
gg.getResults('99999')
gg.editAll('1.1',gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('-1E-45',gg.TYPE_FLOAT)
gg.getResults('99999')
gg.editAll('10000000',gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('0.0001',gg.TYPE_FLOAT)
gg.getResults('99999')
gg.editAll('5000',gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber('5000',gg.TYPE_FLOAT)
gg.getResults('99999')
gg.editAll('0.0001',gg.TYPE_FLOAT)
gg.clearResults()

gg.setVisible(false)
gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("0.40000000596;5.0;0.10000000149:9", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)

gg.refineNumber("0.10000000149", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1, 0)

local t = gg.getResults(99999, nil, nil, nil, nil, nil, nil, nil, nil)
gg.addListItems(t)

gg.clearResults()

local copy = false
local t = gg.getListItems()
if not copy then gg.removeListItems(t) end
for i, v in ipairs(t) do
	v.address = v.address + 0x8
	if copy then v.name = v.name..' #2' end
end
gg.addListItems(t)
t = nil
copy = nil


 gg.clearResults()
  
revert = gg.getListItems()
local t = gg.getListItems()
for i, v in ipairs(t) do
	if v.flags == gg.TYPE_FLOAT then
		v.value = "1.40129846e-45"
		v.freeze = true
		v.freezeType = gg.FREEZE_NORMAL
	end
end
gg.addListItems(t)
t = nil
end
gg.clearResults()
gg.clearList()
 gg.clearResults()
gg.toast("PROGRAM BETA")
function thanks()
gg.toast("DRAG CHANNEL")
gg.sleep(700)
gg.toast("ARE YOU READY")
MainExit()
end

function serv()
gg.setRanges(gg.REGION_ANONYMOUS)
gg.searchNumber("700", gg.TYPE_FLOAT)
gg.getResults(5000)
gg.editAll("999999999999999", gg.TYPE_FLOAT)
gg.clearResults()
gg.searchNumber("700", gg.TYPE_FLOAT)
gg.getResults(5000)
gg.editAll("999999999999999", gg.TYPE_FLOAT)
gg.clearResults()
thanks()
end


