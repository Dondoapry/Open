function lsmenu()
lsm = gg.choice({
"speck 1695",
"speck 2000",

"DRAG"},nil,"BETA")
if lsm == 1 then w2000() end
if lsm == 2 then w16() end
if lsm == 3 then menu() end
end

function w16()
no = gg.choice({
"1695"},nil,"PROGRAM BETA")
if no == 1 then ls1695() end
end

function w2000()
